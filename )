# Є чило перевірити чи воно є парним і двохзначним якщо так то True якщо ні то False 
a=int(input('Введіть число: '))
d=a%2==0 and 9<a<100
print(d)


# Є чило перевірити чи воно НЕ є парним і двохзначним якщо так то True якщо ні то False 
a=int(input('Введіть число: '))
d=a%2!=0 and 9<a<100
print(d)


# Дано 3 цілих числа перевірити чи 2 числа більші 100 якщо так то True якщо ні то False
a=int(input('Введіть 1 число: '))
b=int(input('Введіть 2 число: '))
c=int(input('Введіть 3 число: '))
d=(a>100 and b>100 and c<=100) or (a>100 and b<=100 and c>100) or (a<=100 and b>100 and c>100)
print(d)


#Вводяться натуральні числа доки не введеться число 0
count=0
a=int(input('Введіть число:'))
while a!=0:
    count+=1
    a=int(input('Введіть число:'))
print('Ви ввели',count,'чилел до нуля')


АБО
#Вводяться натуральні числа доки не введеться число 0
count=0
while True:
    a=int(input('Введіть число:'))
    if a==0:
        break
    count+=1
print('Ви ввели',count,'чилел до нуля')


#Вводити речення до тих пір докине буде слово 'Stop' вивести кількість слів до слова 'Stop'
count=0
a=str(input('Введіть слово:'))
while a!='Stop':
    count+=1
    a=str(input('Введіть слово:'))
print('Ви ввели',count,'до Stop')


АБО
#Вводити речення до тих пір докине буде слово 'Stop' вивести кількість слів до слова 'Stop'
count=0
while True:
    a=str(input('Введіть слово:'))
    if a=='Stop':
        break
    count+=1
print('Ви ввели',count,'слів до Stop')


#Вводити додатні числа і рахувати їх суму. Програма зупитняється коли введене відємне число.
sum=0
a=int(input('Введіть число: '))
while a>=0:
    sum=sum+a
    a=int(input('Введіть число: '))
if sum==0:
    print('Ви нічого не ввели!')
else:
    print('Сума до відємного:',sum)


#Вводити додатні числа і рахувати суму тільки додатних. Програма зупитняється коли введене відємне число.
sum=0
a=int(input('Введіть число(>0): '))
while a>=0:
    if a%2==0:
        sum=sum+a
    a=int(input('Введіть число(>0): '))
if sum==0:
    print('Парних чисел немає!')
else:
    print('Сума до відємного:',sum)


#Вклад в банку 10к грн.
#Через кожен рік вклад збільшується на 25%.
#Через суільки місяців віклад буде більшим ніж 15к грн?
s=10000
p=25
mis=0
while s<=15000:
    s=(s//100*25)+s
    mis+=1
print('Через',mis,'місяці')


#Вклад в банку s грн. Якщо вклад>=15000 прога не виконується.
#Через кожен рік вклад збільшується на p%.
#Через скільки місяців вклад буде більшим ніж 15к грн? 
s=float(input('Введіть вклад до 15к: '))
if s>=15000:
    print('Йди запускай змія!')
else:
    p=float(input('Введіть відсоток '))
    mis=0
    while s<=15000:
        s=(s//100*p)+s
        mis+=1
    print('Через',mis,'місяців')


#Вклад в банку s грн. Якщо вклад>=15000 прога не виконується.
#Через кожен рік вклад збільшується на p%.
#Через скільки місяців вклад буде більшим ніж 15к грн? І яка його буде сума? 
s=float(input('Введіть вклад до 15к: '))
if s>=15000:
    print('Йди запускай змія!')
else:
    p=float(input('Введіть відсоток '))
    mis=0
    while s<=15000:
        s=(s//100*p)+s
        mis+=1
    print('Через',mis,'місяців')
    print('Сума після', mis, 'місяців =', s)


#СПИСКИ
lst=[-2, -4, -6, 4, 5, 4, 0]
print(lst)
#сортування списку
lst=sorted(lst)
print(lst)
#мінімальне число списку
print(min(lst))
#максимальне число списку
print(max(lst))
#довжина списку
print(len(lst))
#перший елемент списку(lst=sorted(lst))
print(lst[0])
#останній елемент списку(lst=sorted(lst))
print(lst[6])
#виводиться завжди останній елемент
last=len(lst)
print(lst[last-1])
#додається елемент 100 в кінець списку(lst=sorted(lst))
lst.append(100)
print(lst)
#додається елемент 100 в на 3 місце в списку(lst=sorted(lst))
lst.insert(3,100)
print(lst)
#видалення останнього елементу(100) в списку(lst=sorted(lst))
lst.pop()
print(lst)
#видалення елемету(другого -2) з списку (lst=sorted(lst))
lst.pop(2)
print(lst)
#видалення елемету(другого 100) з списку (lst=sorted(lst))
del lst[2]
print(lst)
#Порахувати кількість чилел 4 з списку (lst=sorted(lst))
print(lst.count(4))
#Надрукувати 50 нулів
a=[0]*50
print(a)


#Написати програму яка виводить цілі числа від 0 до 15. В стовпчик. 
for a in range(0,16): *(з,до,з кроком)
    print(a)


#Написати програму яка виводить цілі числа від 0 до 15, через 5. В стовпчик.
for a in range(1,16,5): 
    print(a)


#Написати програму яка виводить цілі числа від 15 до 1. В стовпчик.
for a in range(15,0,-1): 
    print(a)


#Написати програму яка читає рядок а потім рахує скільки разів треба
#його виводити і вивдить його спільки разів скільки вказано.
s=str(input("Введіть рядок: "))
n=int(input("Введіть кількість рядків: "))
for a in range(n):
    print(s)


#Написати програму яка заповнює список покупок а потім виводить йогов один рядок.
a=['Вода', 'Кава', 'Гречка', 'Грибочки', 'Мандаринки']
print(a)


#Написати програму яка заповнює список покупок а потім виводить його в стовпчик.
a=['Вода', 'Кава', 'Гречка', 'Грибочки', 'Мандаринки']
n=len(a)
for i in range(n):
    print(a[i])


#Ввід списку з клавіатури. В рядок.
n=int(input('Введіть кількість покупок:'))
a=[]
for i in range (n):
    b=input('Введіть назву продукту: ')
    a.append(b)
print(a)

#Ввід списку з клавіатури. В стовпчик.
n=int(input('Введіть кількість покупок:'))
a=[]
for i in range (n):
    b=input('Введіть назву продукту: ')
    a.append(b)
for d in a:
    print(d)


#Ввід списку з клавіатури, кількість елементів списку,
#ціну кожного елементу. Вивід в стовпчик кожен елемент
#з його ціною(в рядок). Потім кінцеву суму.
n=int(input('Введіть кількість покупок:'))
names=[]
price=[]
for i in range (n):
    a=input('Введіть назву продукту: ')
    names.append(a)
    b=float(input('Введіть ціну продукту: '))
    price.append(b)
for i in range(n):
    print(names[i],price[i],'грн')
print('Сума=',sum(price),'грн')


#Видалити з списку перший елемент і пересункти його в кінець.(зсув масиву)
n=int(input('Введіть кількість покупок:'))
names=[]
for i in range (n):
    a=input('Введіть назву продукту: ')
    names.append(a)
b=names.pop(0)
names.append(b)
print(names)


#Пошукова система. Вводиться кількість рядків з даними
#потім самі ці дані. Потім воодиться пошукове слово.
#Потрібно вивести всі рядки де зустрічається пошукове слово.
n=int(input('Кількість рядків:'))
c=[]
for i in range(n):
    b=str(input('Введіть речення:'))
    c.append(b)
poshuk=str(input('Введіть пошукове слово:'))
for s in c:
    if poshuk in s:
        print(s)
